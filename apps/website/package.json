{
  "name": "annaotto-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --watchAll",
    "test:ci": "jest --ci --coverage",
    "lint": "npm run eslint && npm run stylelint",
